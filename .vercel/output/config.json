{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		}
	],
	"images": {
		"domains": [
			"vadrouille.ch"
		],
		"sizes": [
			256,
			384,
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2560
		],
		"formats": [
			"image/avif"
		],
		"remotePatterns": []
	}
}